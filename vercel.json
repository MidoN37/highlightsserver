{
  "version": 2,
  "functions": {
    "api/proxy.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/proxy",
      "dest": "/api/proxy.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node@20"
    }
  ]
} 